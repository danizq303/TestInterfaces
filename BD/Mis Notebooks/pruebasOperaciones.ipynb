    "import numpy as np\n",
    "array = np.arange(5)\n",
    "print(array)\n",
    "print(array[array%2 == 0])\n",
    "\n",
    "y = np.array([10,20,30,15,50])\n",
    "\n",
    "print(y)\n",
    "\n",
    "print(y+array)\n",
    "\n",
    "z = np.where(y > 30)\n",
    "\n",
    "print(z)\n",
    "\n",
    "print(f\"Los numeros mayores que 30 del array y son: {y[z]}\")\n",
    "\n",
    "aMult = np.multiply(array,y)\n",
    "\n",
    "print(aMult)"
